public class Atividade5 {
    public static void main(String[] args) {

        String nomeSmartphone = "ROGphone";
        String marcaSmartphone = "Nvidia";
        double precoSmartphone = 1500.00;
        int quantidadeSmartphone = 20;

        String nomeLaptop = "Asus nitro";
        String marcaLaptop = "Asus";
        double precoLaptop = 2500.00;
        int quantidadeLaptop = 40;

        String nomeFoneBt = "Fone de Ouvido Bluetooth";
        String marca = "Asus";
        double preco = 200.00;
        int quantidade = 10;

        System.out.println("Dados do Smartphone: ");
        System.out.println("Nome: " + nomeSmartphone);
        System.out.println("Marca: " + marcaSmartphone);
        System.out.println("Pre√ßo: " + precoSmartphone);
        System.out.println("Quantidade: " + quantidadeSmartphone);
        System.out.println();
        System.out.println("Dados do Laptop: ");
        System.out.println("Nome: " + nomeLaptop);
        System.out.println("Marca: " + marcaLaptop);
        System.out.println("Preco: " + precoLaptop);
        System.out.println("Quantidade: " + quantidadeLaptop);
        System.out.println();
        System.out.println("Dados do FoneBt: ");
        System.out.println("Nome: " + nomeFoneBt);
        System.out.println("Marca: " + marca);
        System.out.println("Preco: " + preco);
        System.out.println("Quantidade: " + quantidade);
    }
}
